{
	"name": "goncharovei/framework",
	"type": "library",
    "description": "Micro framework",
	"keywords": ["PHP", "console", "MVC"],
    "license": "MIT",
    "authors": [
        {
            "name": "Goncharov Evgeniy"
        }
    ],
    "require": {
        "php": "^8.2",
        "symfony/console": "^7.0.3",
        "monolog/monolog": "^3.8",
        "vlucas/phpdotenv": "^5.6",
        "nesbot/carbon": "^3.8",
        "guzzlehttp/guzzle": "^7.9",
        "illuminate/collections": "^11.38",
        "illuminate/container": "^11.38",
        "symfony/finder": "^7.2",
        "illuminate/config": "^11.39",
        "phpmailer/phpmailer": "^6.9",
        "illuminate/database": "^11.40",
        "twig/twig": "^3.19",
        "guzzlehttp/psr7": "^2.7",
        "league/route": "^6.2",
        "httpsoft/http-emitter": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0.1",
        "symfony/var-dumper": "^7.2",
        "phpstan/phpstan": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "Foundation\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
